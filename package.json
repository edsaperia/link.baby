{
  "name": "serverless-graphql",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:serverless/serverless-graphql.git"
  },
  "scripts": {
    "lint": "eslint .",
    "deploy-prod": "npm install && npm run gen-as && npm run deploy && npm run migrate",
    "deploy": "npx serverless deploy",
    "start": "npm install && concurrently \"npm run start:dev\" \"npm run offline\"",
    "start:dev": "nodemon -L .webpack/main --watch .webpack/main.js",
    "offline": "webpack --config offline.webpack.config.js --watch --progress",
    "gen-as": "node scripts/appsync-mt-gen",
    "migrate": "node migrate",
    "migrate:make": "knex migrate:make --knexfile src/models/knexfile.js",
    "test": "mocha tests/**/*"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "fb": "^2.0.0",
    "iap": "^0.9.1",
    "immutable": "^4.0.0-rc.12",
    "knex": "0.14.6",
    "moment": "^2.22.2",
    "mysql": "2.15.0",
    "oauth": "^0.9.15",
    "objection": "^1.1.10",
    "slack-webhook": "^1.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "apollo-boost": "^0.1.23",
    "apollo-codegen": "^0.19.1",
    "apollo-logger": "^0.3.0",
    "apollo-server-express": "1.3.4",
    "aws-sdk": "^2.176.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "bluebird": "^3.5.1",
    "body-parser": "1.18.3",
    "chai": "^4.1.2",
    "concurrently": "3.6.0",
    "cors": "2.8.4",
    "css-loader": "^2.1.0",
    "ejs": "^2.6.1",
    "env-cmd": "^5.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "express": "4.16.3",
    "graphql-import": "0.6.0",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tag": "2.9.2",
    "graphql-tools": "3.0.2",
    "imports-loader": "^0.7.1",
    "jest": "^21.2.1",
    "mocha": "^5.2.0",
    "mocha-webpack": "^1.1.0",
    "node-fetch": "^1.7.3",
    "node-yaml": "^3.1.1",
    "nodemon": "1.17.5",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "serverless": "^1.32.0",
    "serverless-appsync-plugin": "1.0.0",
    "serverless-offline": "^3.15.3",
    "serverless-plugin-lambda-dead-letter": "^1.2.1",
    "serverless-plugin-warmup": "^3.6.2-rc.1",
    "serverless-s3-sync": "^1.6.1",
    "serverless-stack-output": "0.2.3",
    "serverless-webpack": "^5.1.5",
    "style-loader": "^0.23.1",
    "subscriptions-transport-ws": "^0.9.15",
    "webpack": "^4.0.1",
    "webpack-cli": "^3.0.0",
    "webpack-node-externals": "^1.6.0"
  }
}
